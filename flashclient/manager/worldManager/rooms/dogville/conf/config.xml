<config><!-- DOG VILLE-->
	<assets>
		<asset id="room" path="assets/room.swf" />
	</assets>
	
	<sounds>
		<sound id="roomBackground" type="music" volume="0.4" path="audio/OLO_06_dog_style.mp3" loops="-1" />
	</sounds>
	
	
	<module>
		<elements>
			<element id="helicopter" button="1" visible="1">
				<button type="over" over="4" out="1" />
			</element>
			
			<element id="walkarea" button="1" visible="1">
				<button type="over" over="1" out="1" />
			</element>
			
			<element id="door1" button="1" visible="1">
				<button type="overClick" over="2" out="1" click="1"/>
				<message value="gotoHotSpot" >
					<hotSpot value="door1" />
				</message>
			</element>
			
			<element id="door2" button="1" visible="1">
				<button type="overClick" over="2" out="1" click="1"/>
				<message value="gotoHotSpot" >
					<hotSpot value="door2" />
				</message>
			</element>
		
		</elements>
		
		<hotspots>
			<hotspot target="19" 		type="room" 			sharetype="unique" shape="round" x="422" y="330" rad="30" dirAngle="-35"/> <!-- big tree -->
			<hotspot target="7" 		type="room" 			sharetype="unique" shape="round" x="743" y="681" rad="50" dirAngle="109"/> <!-- food corner -->
			<hotspot target="17" 		type="room" 			sharetype="unique" shape="round" x="897" y="576" rad="50" dirAngle="20"/> <!-- montecielo -->
			<hotspot target="12" 		type="room" 			sharetype="unique" shape="round" x="67" y="682" rad="50" dirAngle="140"/> <!-- texa relaxa -->
			<hotspot target="1000" type="home" 			sharetype="unique" shape="round" x="866" y="455" rad="50" dirAngle="-45"/> <!-- home -->
		</hotspots>
		<scale>
			<value minScale="60" maxScale="70"/>
		</scale>
		<faces>
			<face name="wiseTree"/>
		</faces>
	</module>
</config>
